##############################################################
## HLS Module "mac"
##############################################################


#
# Dependency Generation rule for BEH module
#
$(BDW_OBJDIR)/mac.d :  ../src/mac.cpp
	@if [ ! -d $(BDW_OBJDIR) ]; then mkdir -p $(BDW_OBJDIR); fi
	@echo "Generating dependencies for $< "
	@set -e; $(BDW_CCDEP) -MM -MG $(BDW_CCFLAGS)  ../src/mac.cpp \
	| sed 's|mac\.o[ :]*|$(BDW_OBJDIR)/mac.o $(BDW_OBJDIR)/mac.d : |' $(BDW_DEP_FILTER)  > $@

$(BDW_WORKLIB)/wrappers/mac_trace.h : $(BDW_WORKLIB)/wrappers/mac_trace.h.updated

$(BDW_WORKLIB)/wrappers/mac_trace.h.updated :  ../src/mac.cpp $(BDW_LOGOPTIONS_FILE)
	$(STRATUS_HOME)/bin/bdw_tracegen -module mac  
	@touch $(BDW_WORKLIB)/wrappers/mac_trace.h.updated



$(BDW_WORKLIB)/objs/incisive/mac.o:  ../src/mac.cpp $(BDW_WORKLIB)/wrappers/mac_wrap.cpp $(BDW_WORKLIB)/wrappers/mac_wrap.h $(BDW_WORKLIB)/wrappers/mac_cosim.h $(BDW_WORKLIB)/wrappers/mac_trace.h 
	@if [ ! -d $(BDW_OBJDIR) ]; then mkdir -p $(BDW_OBJDIR); fi
	$(BDW_CC) " $(BDW_CCFLAGS) -DioConfig_IOCFG_DMA32 -D_p_ioConfig_IOCFG_DMA32 -DioConfig=IOCFG_DMA32 -D_wrap_ioConfig=IOCFG_DMA32 -DDMA_WIDTH=32 -D_wrap_DMA_WIDTH=32  -o $(BDW_WORKLIB)/objs/incisive/mac.o " $(BDW_WORKLIB)/wrappers/mac_wrap.cpp

.INTERMEDIATE : mac_wrap.h

mac_wrap.h : $(BDW_WORKLIB)/wrappers/mac_wrap.h

$(BDW_WORKLIB)/wrappers/mac_wrap.cpp $(BDW_WORKLIB)/wrappers/mac_wrap.h $(BDW_WORKLIB)/wrappers/mac_cosim.h $(BDW_WORKLIB)/wrappers/mac_cosim.v   : $(BDW_WRAPDIR)/mac.updated

$(BDW_WRAPDIR)/mac.updated :  ../src/mac.cpp 
	@if [ ! -d $(BDW_WRAPDIR) ]; then mkdir -p $(BDW_WRAPDIR); fi
	$(STRATUS_HOME)/bin/bdw_wrapgen -project project.tcl -module mac "-I$(BDW_WRAPDIR) $(BDW_LIB_DASHI) $(BDW_CCOPTIONS) $(BDW_EXTRA_CCFLAGS)"
	@touch $(BDW_WRAPDIR)/mac.updated
	$(STRATUS_HOME)/bin/bdw_tracegen -module mac  
	@touch $(BDW_WORKLIB)/wrappers/mac_trace.h.updated
